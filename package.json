    "moment": "^2.29.1",
    "mongodb": "^3.6.6",
    "mssql": "^6.3.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "uuid": "^8.3.2",
    "validatorjs": "^3.22.1"
